micros:
  - name: frontend
    src: frontend
    engine: static
    primary: true
    commands:
      - npm run build
    serve: dist
    dev: npm run dev -- --port $PORT
  - name: backend
    src: backend
    engine: nodejs16
    run: node index.js
    path: api
    dev: node index.js


